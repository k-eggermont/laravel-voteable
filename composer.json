{
    "name": "keggermont/voteable",
    "description": "Voteable trait for laravel 5.5",
    "authors": [
        {
            "name": "keggermont",
            "email": "kevin.eggermont@gmail.com"
        }
    ],

    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.* || 5.6.*",
        "illuminate/database": "5.5.* || 5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Keggermont\\Voteable\\": "src/"
        }
    },
    "autoload-dev": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Keggermont\\Voteable\\VoteableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
